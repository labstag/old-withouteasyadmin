{

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true
    },
    "search.exclude": {
        "**/apps/var": true,
        "**/apps/vendor": true,
        "**/node_modules": true
    },
    "files.watcherExclude": {
        "**/apps/var": true,
        "**/apps/vendor": true,
        "**/node_modules": true
    },
    "intelephense.files.exclude": [
        "**/.git/**",
        "**/.svn/**",
        "**/.hg/**",
        "**/CVS/**",
        "**/.DS_Store/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/vendor/**/{Tests,tests}/**",
        "**/.history/**",
        "**/vendor/**/vendor/**",
        "**/apps/var/**",
    ],
    "php.files.exclude": [
        "**/node_modules/**"
        "**/apps/var/**"
    ],
    "php.executablePath": "/bin/php"
}