{% extends 'base.html.twig' %}
{% block stylesheets %}
    {{ parent() }}
    {{ encore_entry_link_tags('front') }}
    {% if config.tarteaucitron is defined %}
        <link rel="stylesheet" href="{{ asset('assets/tarteaucitron/css/tarteaucitron.css') }}">
    {% endif %}
{% endblock %}
{% block javascripts %}
    {{ parent() }}
    {{ encore_entry_script_tags('front') }}
    {% if config.tarteaucitron is defined %}
        <script type="text/javascript" src="{{ asset('assets/tarteaucitron/tarteaucitron.js') }}"></script>
        <script type="text/javascript">
    tarteaucitron.init(
    {{ configtarteaucitron|json_encode(constant('JSON_PRETTY_PRINT'))|raw }}
    );
    tarteaucitron.job = tarteaucitron.job || [];
    {% if config.tarteaucitron.job is defined %}
{{ config.tarteaucitron.job|raw }}
    {% endif %}
        </script>
    {% endif %}
{% endblock %}
{% block content %}
    {% for row, blockhtml in blocks %}
        <{{ row }}>
            {% for row in blockhtml %}
                {{ row.template|debug_begin }}
                {{ row.data.content|raw }}
                {{ row.template|debug_end }}
            {% endfor %}
        </{{ row }}>
    {% endfor %}
{% endblock %}
